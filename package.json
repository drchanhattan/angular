{
  "name": "website",
  "version": "0.0.0",
  "scripts": {
    "storybook": "ng run website:storybook",
    "build-storybook": "ng run website:build-storybook",
    "delete-storybook": "rmdir /s /q public\\storybook-static",
    "move-storybook": "rmdir /s /q public\\storybook-static && xcopy /E /I /Y storybook-static public\\storybook-static && rmdir /s /q storybook-static",
    "404": "copy docs\\index.html docs\\404.html",
    "locate-circular-deps": "npx madge --circular --extensions ts ./",
    "prettier": "npx prettier --write \"**/*.ts\" \"**/*.html\"",
    "build": "ng build --configuration production --output-path docs --base-href .",
    "docs": "robocopy docs\\browser docs /E /MOVE && rmdir /s /q docs\\browser || exit 0",
    "cname": "echo christopherchan.co.uk > docs\\CNAME",
    "commit": "git add . && git commit -m \"commit\"",
    "push": "git push -u origin master",
    "upload": "npm run prettier && npm run build && npm run docs && npm run 404 && npm run cname && npm run commit && npm run push"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.6",
    "@angular/cdk": "^20.0.5",
    "@angular/common": "^20.0.6",
    "@angular/compiler": "^20.0.6",
    "@angular/core": "^20.0.6",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.0.6",
    "@angular/material": "^20.0.5",
    "@angular/platform-browser": "^20.0.6",
    "@angular/platform-browser-dynamic": "^20.0.6",
    "@angular/router": "^20.0.6",
    "aos": "^2.3.4",
    "firebase": "^10.12.3",
    "obscenity": "^0.3.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typescript": "^5.8.3",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.6",
    "@angular/build": "^20.0.5",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "^20.0.6",
    "@compodoc/compodoc": "^1.1.26",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-onboarding": "^9.0.17",
    "@storybook/addon-styling-webpack": "^2.0.0",
    "@storybook/angular": "^9.0.17",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/aos": "^3.0.7",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.19",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "storybook": "^9.0.17",
    "tailwindcss": "^4.1.11"
  }
}
